<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.raindrop.screenshot.automation</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/node</string>
        <string>watcher.js</string>
    </array>
    
    <key>WorkingDirectory</key>
    <string>/Users/tejas/Documents/Raindrop%20SS%20Automation</string>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>KeepAlive</key>
    <true/>
    
    <key>StandardOutPath</key>
    <string>/Users/tejas/Library/Logs/RaindropScreenshot.log</string>
    
    <key>StandardErrorPath</key>
    <string>/Users/tejas/Library/Logs/RaindropScreenshot.error.log</string>
    
